<p id="notice"><%= notice %></p>

<h1>Privilieges</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @privilieges.each do |priviliege| %>
      <tr>
        <td><%= link_to 'Show', priviliege %></td>
        <td><%= link_to 'Edit', edit_priviliege_path(priviliege) %></td>
        <td><%= link_to 'Destroy', priviliege, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Priviliege', new_priviliege_path %>
